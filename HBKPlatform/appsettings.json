{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "None",
      "Npgsql.Command": "Information"
    },
    "File": {
      "BasePath": "log",
      "FileAccessMode": "KeepOpenAndAutoFlush",
      "FileEncodingName": "utf-8",
      "DateFormat": "yyyyMMdd",
      "CounterFormat": "000",
      "MaxFileSize": 10485760,
      "LogLevel": {
        "Default": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "None",
        "Npgsql.Command": "None"
      },
      "IncludeScopes": true,
      "MaxQueueSize": 100,
      "Files": [
        {
          "Path": "<date:yyyy>/<date:MM>/error-<counter>.log"
        }
      ]
    },
